make_col path:lower(string(EXTRA.path))
filter contains(string(path), "firewall")

make_table
pick_col
    LogSource:string("Firewall Log"), 
    Date:string(FIELDS.date),
    Time:string(FIELDS.time)

aggregate group_by(LogSource),
    StartDate:min(Date),
    EndDate:max(Date),
    EarliestTime:min(Time),
    LatestTime:max(Time),
    UniqueDates:count_distinct(Date),
    LogCount:count()
